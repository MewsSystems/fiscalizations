<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../../Mews.Fiscalizations.All.props" />
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <LangVersion>7.3</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageOutputPath>../../Mews.Fiscalizations.All/bin/$(Configuration)/</PackageOutputPath>
    <Authors>Mews Systems</Authors>
    <Company>Mews Systems</Company>
    <PackageTags>fiscalization greece mydata</PackageTags>
    <PackageProjectUrl>https://github.com/MewsSystems/fiscalization-greece</PackageProjectUrl>
    <License>https://github.com/MewsSystems/fiscalization-greece/blob/master/LICENSE</License>
    <RepositoryUrl>https://github.com/MewsSystems/fiscalization-greece</RepositoryUrl>
    <Description>Client for MyData - Greek e-books platform.</Description>
    <Icon>https://raw.githubusercontent.com/msigut/eet/master/receipt.png</Icon>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FuncSharp">
      <HintPath>..\..\packages\FuncSharp.3.0.1\lib\netstandard2.0\FuncSharp.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Core\Mews.Fiscalization.Core\Mews.Fiscalizations.Core.csproj" />
  </ItemGroup>

  <PropertyGroup>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>
  
</Project>
