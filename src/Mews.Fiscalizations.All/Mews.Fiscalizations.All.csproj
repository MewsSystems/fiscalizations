<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Authors>Mews</Authors>
    <Copyright>Mews</Copyright>
    <PackageProjectUrl>https://github.com/MewsSystems/fiscalization-all</PackageProjectUrl>
    <License>https://github.com/MewsSystems/fiscalization-all/blob/master/LICENSE</License>
    <RepositoryUrl>https://github.com/MewsSystems/fiscalization-all</RepositoryUrl>
    <Icon>https://raw.githubusercontent.com/msigut/eet/master/receipt.png</Icon>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>fiscalizations</PackageTags>
    <Description>All Mews Fiscalizations</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Mews.Fiscalization.Austria\Mews.Fiscalization.Austria.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Core\Mews.Fiscalization.Core.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Czechia\Mews.Fiscalization.Czechia.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Germany\Mews.Fiscalization.Germany.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Greece\Mews.Fiscalization.Greece.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Italy\Mews.Fiscalization.Italy.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Spain\Mews.Fiscalization.Spain.csproj" />
    <ProjectReference Include="..\Mews.Fiscalization.Uniwix\Mews.Fiscalization.Uniwix.csproj" />
  </ItemGroup>
  
  <ItemGroup>
    <Content Include="packages.config">
      <SubType>Designer</SubType>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FuncSharp">
      <HintPath>..\packages\FuncSharp.3.0.1\lib\netstandard2.0\FuncSharp.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <PropertyGroup>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

</Project>
