<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../Mews.Fiscalizations.All.props" />
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Authors>Mews</Authors>
    <Description>All Mews Fiscalizations</Description>
    <PackageTags>fiscalizations</PackageTags>
    <PackageProjectUrl>https://github.com/MewsSystems/fiscalizations</PackageProjectUrl>
    <License>https://github.com/MewsSystems/fiscalizations/blob/master/LICENSE</License>
    <RepositoryUrl>https://github.com/MewsSystems/fiscalizations</RepositoryUrl>
    <Icon>https://raw.githubusercontent.com/msigut/eet/master/receipt.png</Icon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  
  <ItemGroup>
    <Content Include="packages.config">
      <SubType>Designer</SubType>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FuncSharp">
      <HintPath>..\packages\FuncSharp.3.0.1\lib\netstandard2.0\FuncSharp.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Austria\Mews.Fiscalization.Austria\Mews.Fiscalizations.Austria.csproj" />
    <ProjectReference Include="..\Czechia\Mews.Fiscalization.Czechia\Mews.Fiscalizations.Czechia.csproj" />
    <ProjectReference Include="..\Germany\Mews.Fiscalization.Germany\Mews.Fiscalizations.Germany.csproj" />
    <ProjectReference Include="..\Greece\Mews.Fiscalization.Greece\Mews.Fiscalizations.Greece.csproj" />
    <ProjectReference Include="..\Hungary\Mews.Fiscalization.Hungary\Mews.Fiscalizations.Hungary.csproj" />
    <ProjectReference Include="..\Italy\Mews.Fiscalization.Italy\Mews.Fiscalizations.Italy.csproj" />
    <ProjectReference Include="..\Italy\Mews.Fiscalization.Uniwix\Mews.Fiscalizations.Uniwix.csproj" />
    <ProjectReference Include="..\Spain\Mews.Fiscalization.Spain\Mews.Fiscalizations.Spain.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
  <PropertyGroup>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>
</Project>
